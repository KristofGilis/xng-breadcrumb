    "@nx/workspace": "16.2.2",
    "@schematics/angular": "~16.0.0",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.4.0",
    "@types/node": "16.11.7",
    "@typescript-eslint/eslint-plugin": "^5.58.0",
    "@typescript-eslint/parser": "^5.58.0",
    "all-contributors-cli": "^6.25.1",
    "cypress": "^12.11.0",
    "danger": "^11.2.6",
    "eslint": "~8.15.0",
